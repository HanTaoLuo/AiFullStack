import json
def format_rednote_for_markdown(json_string:str)->str:
    """
    å°† JSON æ ¼å¼çš„å°çº¢ä¹¦æ–‡æ¡ˆè½¬æ¢æˆmarkdown æ ¼å¼ï¼Œä»¥ä¾¿é˜…è¯»å’Œå‘å¸ƒã€‚
    :param json_string: åŒ…å«å°çº¢ä¹¦æ–‡æ¡ˆçš„ JSON å­—ç¬¦ä¸²ã€‚ æ ¼å¼ä¸º {"title": "...", "body": "...", "hashtags": [...], "emojis": [...]}
    :return:
    """
    try:
        data = json.loads(json_string)
    except Exception as e:
        return f"æ— æ³•è§£æ json å­—ç¬¦ä¸² å¼‚å¸¸ä¸º:{e} \n åŸå§‹å­—ç¬¦ä¸²ä¸ºï¼š{json_string}"
    # è·å–æ ‡é¢˜ã€å†…å®¹ã€æ ‡ç­¾
    title = data.get("title", "æ— æ ‡é¢˜")
    body = data.get("body", "")
    hashtags = data.get("hashtags", [])
    # è¡¨æƒ…ç¬¦å·é€šå¸¸å·²ç»èå…¥æ ‡é¢˜å’Œæ­£æ–‡ä¸­ï¼Œè¿™é‡Œå¯ä»¥é€‰æ‹©æ˜¯å¦å•ç‹¬åˆ—å‡º
    emojis = data.get("emojis", [])
    # æ„å»º markdown æ–‡æœ¬
    # æ ‡é¢˜æ„å»ºä¸ºäºŒçº§æ ‡é¢˜
    markdown_output = f"## {title} \n\n"
    # æ„å»ºæ­£æ–‡éƒ¨åˆ† ä¿ç•™æ¢è¡Œç¬¦
    markdown_content = f"{body} \n\n"
    markdown_emoji= ""
    markdown_hashtag = ""
    # æ„å»ºæ ‡ç­¾
    if hashtags:
        # å°çº¢ä¹¦æ ‡ç­¾é€šå¸¸ä»¥ç©ºæ ¼åˆ†éš”
        markdown_hashtag =  " ".join(hashtags)

    # å¦‚æœéœ€è¦,å¯ä»¥å•ç‹¬åˆ—å‡º emoji è¡¨æƒ…ï¼Œä½†æ˜¯é€šå¸¸å·²ç»åŒ…å«åœ¨æ ‡é¢˜å’Œæ­£æ–‡ä¸­
    if emojis:
        emoji_string =  "".join(emojis)
        markdown_emoji = emoji_string
    markdown_output+=markdown_content
    markdown_output+=markdown_hashtag
    markdown_output+=markdown_emoji
    # å»æ‰æœ«å°¾å¤šä½™çš„ç©ºç™½
    return markdown_output.strip()



