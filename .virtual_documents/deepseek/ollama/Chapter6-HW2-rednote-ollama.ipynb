import re

def generate_redbook_note(product_name:str,tone_style:str="æ´»æ³¼ç”œç¾",max_iterations:int = 5)->str:
    """
    ä½¿ç”¨Deepseek Agent ç”Ÿæˆå°çº¢ä¹¦çˆ†æ¬¾æ–‡æ¡ˆ
    :param product_name: è¦ç”Ÿæˆçš„äº§å“åç§°
    :param tone_style: æ–‡æ¡ˆçš„è¯­æ°”å’Œé£æ ¼ï¼Œå¦‚"æ´»æ³¼å¯çˆ±"ã€"çŸ¥æ€§"ã€"ææ€ª"ç­‰
    :param max_iterations:  Agent æœ€å¤§è¿­ä»£æ¬¡æ•°
    :return: ç”Ÿæˆçˆ†æ¬¾æ–‡æ¡ˆ(JSON æ ¼å¼å­—ç¬¦ä¸²)
    """
    print(f"\n å¯åŠ¨å°çº¢ä¹¦æ–‡æ¡ˆç”ŸæˆåŠ©æ‰‹ äº§å“:{product_name} ,  æ–‡æ¡ˆé£æ ¼:{tone_style}")
    # json {{}}  è¡¨ç¤º éè½¬ä¹‰å­—ç¬¦ æ­£å¸¸è¾“å‡º{}   eg: {{....}} è¾“å‡º {....}
    USER_PROMPT={
        "role":"user",
        "content":f"è¯·ä¸ºäº§å“ã€Œ{product_name}ã€ç”Ÿæˆä¸€ç¯‡å°çº¢ä¹¦çˆ†æ¬¾æ–‡æ¡ˆã€‚è¦æ±‚ï¼šè¯­æ°”{tone_style}ï¼ŒåŒ…å«æ ‡é¢˜ã€æ­£æ–‡ã€è‡³å°‘5ä¸ªç›¸å…³æ ‡ç­¾å’Œ5ä¸ªè¡¨æƒ…ç¬¦å·ã€‚è¯·ä»¥å®Œæ•´çš„JSONæ ¼å¼è¾“å‡ºï¼Œå¹¶ç¡®ä¿JSONå†…å®¹ç”¨markdownä»£ç å—åŒ…è£¹ï¼ˆä¾‹å¦‚ï¼š```json{{...}}```ï¼‰ã€‚"
    }
    messages = [
        SYSTEM_PROMPT,
        USER_PROMPT
    ]
    iteration_count = 0
    
    while iteration_count<max_iterations:
        iteration_count+=1
        print(f"-- Iteration {iteration_count} --")
        try:
            # è°ƒç”¨ Deepseek APIï¼Œä¼ å…¥å¯¹è¯å†å²å’Œå·¥å…·å®šä¹‰
            response  = client.chat.completions.create(
                model = deepseek_model,
                messages = messages,
                tools=TOOLS_DEFINITION,
                tool_choice="auto"
            )
            response_message = response.choices[0].message

            # **ReAct æ¨¡å¼:å¤„ç†å·¥å…·è°ƒç”¨**
            # å¦‚æœæ¨¡å‹å†³å®šè°ƒç”¨å·¥å…·
            if response_message.tool_calls:
                print(f"Agent å†³å®šè°ƒç”¨å·¥å…· .....")
                #å°†å·¥å…·è°ƒç”¨ä¿¡æ¯æ·»åŠ åˆ°å¯¹è¯å†å²ä¸­ è¡¨ç¤ºmessage å·²ç»ç”±æ¨¡å‹è°ƒç”¨äº†å·¥å…·ã€‚ ä¹Ÿå°±æ˜¯æ·»åŠ å·¥å…·è°ƒç”¨ä¸Šä¸‹æ–‡
                messages.append(response_message)
                tools_outputs = []
                # éå†å¤§æ¨¡å‹è¦è°ƒç”¨çš„æ‰€æœ‰å·²æ³¨å†Œå‡½æ•°
                for tool_call in response_message.tool_calls:
                    function_name = tool_call.function.name
                    # ä¿è¯è°ƒç”¨å‡½æ•°çš„å‚æ•°ä¸ºåˆæ³•çš„ json å­—ç¬¦ä¸²
                    # å³ä¾¿å·¥å…·ä¸è¦æ±‚ä½¿ç”¨å‚æ•°  ä¹Ÿéœ€è¦ä¼ å…¥ç©ºçš„å­—å…¸ï¼
                    function_args =  json.loads(tool_call.function.arguments) if tool_call.function.arguments else {}
                    print(f"Agent è°ƒç”¨å·¥å…·ï¼š  è°ƒç”¨å·¥å…·(function) åç§°ä¸º:{function_name}  å¯¹åº” tools çš„ å‚æ•°ä¸º {function_args}")

                    # è°ƒç”¨å·¥å…·
                    if function_name in available_tools:
                        # æ ¹æ®å®šä¹‰çš„åå­—æ‰¾åˆ°å¯¹åº”å‡½æ•°å¯¹è±¡
                        tool_function = available_tools[function_name]
                        # åç§°-å‡½æ•°æ˜ å°„è¡¨ è°ƒç”¨å‡½æ•°
                        tool_result = tool_function(**function_args)
                        print(f"Observation : å·¥å…· {function_name} è¿”å›ç»“æœ{tool_result}")
                        # æ‹¼æ¥å‡½æ•°è¾“å‡ºç»“æœã€‚ä¾›è¿”å›ç»™å¤§æ¨¡å‹  æ¨¡å‹å¯ä»¥æ ¹æ®è°ƒç”¨ id çŸ¥é“è°ƒç”¨äº†ä»€ä¹ˆå·¥å…·
                        tools_output = {
                            "tool_call_id":tool_call.id,
                            "role":"tool",
                            "content":str(tool_result)
                        }
                        tools_outputs.append(tools_output)
                    else:
                        error_msg = f"æœªæ³¨å†Œå·¥å…· {function_name} è¢«è°ƒç”¨ï¼"
                        print(error_msg)
                        tools_output= {
                            "tool_call_id":tool_call.id,
                            "role":"tool",
                            "content":error_msg
                        }
                        tools_outputs.append(tools_output)
                # å°†æ¨¡å‹è°ƒç”¨å‡½æ•°ç”Ÿæˆç»“æœæ‹¼æ¥å›æ¶ˆæ¯ä½“ä¸­
                messages.extend(tools_outputs)
            # å½“æ¨¡å‹ç›´æ¥è¿”å›/è¾“å‡ºå†…å®¹çš„æ—¶å€™ è¡¨ç¤ºç”Ÿæˆäº†æœ€ç»ˆç­”æ¡ˆ(  ä¸€èˆ¬æ¥è¯´æ˜¯è¿™æ ·)
            # **ReAct æ¨¡å¼:å¤„ç†æœ€ç»ˆç»“æœ**
            elif response_message.content:
                print(f"æ¨¡å‹ç”Ÿæˆç»“æœ:{response_message.content}")
                # ç”±äº USER_PROMPT è¦æ±‚ç”Ÿæˆæ–‡æœ¬ä¸º json çš„ markdown  æå–ç”Ÿæˆçš„ json å†…å®¹
                # re.DOTALL è¡¨ç¤ºæ”¹å˜æ­£åˆ™è¡¨è¾¾å¼ç‚¹å·(.)çš„é»˜è®¤åŒ¹é…è¡Œä¸ºï¼Œ é»˜è®¤.å·è¡¨ç¤ºåŒ¹é…é™¤æ¢è¡Œç¬¦ï¼ˆ\nï¼‰ä»¥å¤– çš„ä»»æ„å­—ç¬¦
                # å¯ç”¨ re.DOTALL æ ‡å¿—æ—¶"." ä¼šåŒ¹é…åŒ…æ‹¬æ¢è¡Œç¬¦ï¼ˆ\nï¼‰åœ¨å†…çš„æ‰€æœ‰å­—ç¬¦
                json_string_match =  re.search(r"```json\s*(\{.*\})\s*```",response_message.content,re.DOTALL)
                if json_string_match:
                    # æå–åŒ¹é…ç”Ÿæˆå†…å®¹
                    extracted_json_content = json_string_match.group(1)
                    try:
                        final_response = json.loads(extracted_json_content)
                        print(f"Agent:ä»»åŠ¡å®Œæˆ,ç”Ÿæˆè§£ææœ€ç»ˆçš„ JSON æ–‡æ¡ˆï¼")
                        return json.dumps(final_response,ensure_ascii=False,indent=2)
                    except Exception as e:
                        print(f"Agent:æå–åˆ° json ä½†æ˜¯è§£æå¤±è´¥:{e}")
                        print(f"å°è¯•è§£æçš„ json å­—ç¬¦ä¸²ä¸º\n :{extracted_json_content}")
                        # è§£æå¤±è´¥ ç»§ç»­å¯¹è¯
                        messages.append(response_message)
            else:
                print("Agent : æœªçŸ¥å¼‚å¸¸,å¯èƒ½éœ€è¦æ›´å¤šäº¤äº’ï¼")

        except Exception as e:
            print(f"è°ƒç”¨ DeepSeek API æ—¶å‘ç”Ÿé”™è¯¯: {e}")
            break
    print("\n Agent è¾¾åˆ°æœ€å¤§è¿­ä»£æ¬¡æ•°æˆ–æœªèƒ½ç”Ÿæˆæœ€ç»ˆæ–‡æ¡ˆã€è¯·æ£€æŸ¥ Promptæˆ–å¢åŠ è¿­ä»£æ¬¡æ•°ã€‚")
    return "æœªèƒ½ç”Ÿæˆæœ€ç»ˆæ–‡æ¡ˆï¼"






